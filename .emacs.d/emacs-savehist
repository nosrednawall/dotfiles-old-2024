# Imports
import os
from math import *
from string import *
import time
import threading

# Funcoes
def calcula_raiz_numero(numero):
    return sqrt(numero)


def e_primo(numero):
    ePrimo = False
    strNumero = str(numero)
    soma = 0
    divisivelPor5 = 1

    if(strNumero[-1] in ['5','0']):
        divisivelPor5 = numero % 5

    for i in range(len(strNumero)):
        soma += int(strNumero[i])

    if(numero == int(2)):
        ePrimo = True
    elif(numero == 1 or numero == 0):
        ePrimo = False
    elif(soma % 3 == 0):
        ePrimo = False
    elif(divisivelPor5 == int(0)):
        ePrimo = False
    elif(strNumero[-1] in ['2','4','6','8','0']):
        ePrimo = False
    else:
        fatorial = []
        menorValorChecagem = int(sqrt(numero))

        for divisor in range(2, menorValorChecagem +1):
            resto = numero % divisor
            if(resto == 0):
                fatorial.append(divisor)

        if((len(fatorial) == int(0))):
            ePrimo = True

    return ePrimo


def processo_calcula_soma():

    # Definicao das variaveis
    numeroValidoInformado = False
    qtdeNumerosInformados = 0
    print('Bem vindo ao Programa Calcula soma!!')

    while(not numeroValidoInformado):
        print('Informe a quantidade de números que deseja calcular a soma,'
        '\\n A Quantidade mímina é de 2')

        qtdeNumerosInformados=str(input('Digite a qtde: '))
        if(not qtdeNumerosInformados.isnumeric()):
            print('Valor Inválido!\\n\\n')
            qtdeNumerosInformados = 0

        elif(int(qtdeNumerosInformados) < 2):
            print('Qtde inválida!\\n\\n')
            qtdeNumerosInformados = 0

        else:
            numeroValidoInformado = True
            listaNumerosSoma = []
            qtdeNumerosInformados = int(qtdeNumerosInformados)

            for indice in range(1,qtdeNumerosInformados+1):
                numeroValido = False

                while(not numeroValido):
                    numeroSoma = str(input('Informe o seu '+str(indice)+' numero '
                                           'para realizar a soma: '))

                    if(not numeroRaiz.isnumeric()):
                        print('Número Inválido!')
                        numeroSoma = 0
                    else:
                        numeroValido = True

                listaNumerosSoma.append(float(numeroSoma))

    print('\\n\\nSegue a soma dos números ',*listaNumerosSoma, ' é de:',
          round(sum(listaNumerosSoma), 2))


def processo_calcula_media():

    # Definicao das variaveis
    numeroValidoInformado = False
    qtdeNumerosInformados = 0
    print('Bem vindo ao Programa Calcula médias!!')

    while(not numeroValidoInformado):
        print('Informe a quantidade de números que deseja calcular a média,'
        '\\n A Quantidade mímina é de 2')

        qtdeNumerosInformados=str(input('Digite a qtde: '))
        if(not qtdeNumerosInformados.isnumeric()):
            print('Valor Inválido!\\n\\n')
            qtdeNumerosInformados = 0

        elif(int(qtdeNumerosInformados) < 2):
            print('Qtde inválida!\\n\\n')
            qtdeNumerosInformados = 0

        else:
            numeroValidoInformado = True
            listaNumerosMedias = []
            qtdeNumerosInformados = int(qtdeNumerosInformados)

            for indice in range(1,qtdeNumerosInformados+1):
                numeroValido = False

                while(not numeroValido):
                    numeroMedia = str(input('Informe o seu '+str(indice)+' numero '
                                           'para calcular a raiz: '))

                    if(not numeroMedia.isnumeric()):
                        print('Número Inválido!')
                        numeroMedia = 0
                    else:
                        numeroValido = True

                listaNumerosMedias.append(float(numeroMedia))

    print('Segue a média dos números', *listaNumerosMedias,
          ' é de:',round((sum(listaNumerosMedias)/float(qtdeNumerosInformados)),2))


def processo_calcula_raiz():

    # Definicao das variaveis
    numeroValidoInformado = False
    qtdeNumerosInformados = 0
    print('Bem vindo ao Programa Calcula  raizes!!')

    while(not numeroValidoInformado):
        print('Informe a quantidade de números que deseja calcular a raiz,'
        '\\n A Quantidade mímina é de 3')

        qtdeNumerosInformados=str(input('Digite a qtde: '))
        if(not qtdeNumerosInformados.isnumeric()):
            print('Valor Inválido!\\n\\n')
            qtdeNumerosInformados = 0

        elif(int(qtdeNumerosInformados) < 3):
            print('Qtde inválida!\\n\\n')
            qtdeNumerosInformados = 0

        else:
            numeroValidoInformado = True
            listaNumerosRaizes = []
            qtdeNumerosInformados = int(qtdeNumerosInformados)

            for indice in range(1,qtdeNumerosInformados+1):
                numeroValido = False

                while(not numeroValido):
                    numeroRaiz = str(input('Informe o seu '+str(indice)+' numero '
                                           'para calcular a raiz: '))

                    if(not numeroRaiz.isnumeric()):
                        print('Número Inválido!')
                        numeroRaiz = 0
                    else:
                        numeroValido = True

                raiz = round(calcula_raiz_numero(float(numeroRaiz)), 2)
                listaNumerosRaizes.append([numeroRaiz,raiz])

    print('Segue a raiz dos números solicitados:')

    for numero in listaNumerosRaizes:
        print('   - Número ',numero[0],' possui a raiz ',numero[1])


def processo_calcula_numeros_primos():
    # Definicao das variaveis
    inicio = time.time()
    numeroValidoInformado = False
    qtdeNumerosInformados = 0
    path = '/home/anderson/git/Educacional/UFPR_Estatistica/'
    'CI182_Fundamentos_Programacao_Computadores'

    print('Bem vindo ao Programa Calcula numeros primos!!')

    while(not numeroValidoInformado):
        print('Informe a quantidade de números primos que deseja calcular,'
        '\\n A Quantidade mímina é de 3')

        qtdeNumerosInformados=str(input('Digite a qtde: '))
        if(not qtdeNumerosInformados.isnumeric()):
            print('Valor Inválido!\\n\\n')
            qtdeNumerosInformados = 0

        elif(int(qtdeNumerosInformados) < 3):
            print('Qtde inválida!\\n\\n')
            qtdeNumerosInformados = 0

        else:
            numeroValidoInformado = True
            listaNumerosPrimos = []
            qtdeNumerosInformados = int(qtdeNumerosInformados)

            numeroPrimo = 0
            finalizado = False
            limite = int(qtdeNumerosInformados)/200
            contador = 0
            with open('numeros_primos.txt', 'a') as arquivo:
                while(not finalizado):
                    numeroPrimo+=1
                    if(e_primo(numeroPrimo)):
                        listaNumerosPrimos.append(int(numeroPrimo))
                        if(contador >= limite):
                            #arquivo.write(str(listaNumerosPrimos))
                            #listaNumerosPrimos = []
                            contador = 0
                        if(len(listaNumerosPrimos) >= qtdeNumerosInformados):
                                finalizado = True
                    contador+=1
                arquivo.write(str(listaNumerosPrimos))

    fim = time.time()
    print('Os números primos foram salvos no arquivo ',path,'/numeros_primos.txt')
    print(\"Tempo decorrido: \", fim - inicio)


# Inicio do programa
if __name__ == \"__main__\":
    print('Selecione um número referente as funções abaixo:')
    print('  - 1 para Calcular soma\\n'
          '  - 2 para Calcular média aritimética\\n'
          '  - 3 para Calcular raiz\\n'
          '  - 4 para Calcular números primos\\n'
          '  - 0 para sair')

    escolhaValida = False
    escolha = 0
    while(not escolhaValida):
        escolha = str(input('Selecione: '))

        if(not escolha.isnumeric()):
            print('Número inválido!\\n\\n')
            escolha = 0
        else:
            escolhaValida = True

    sair = False
    while(not sair):
        if int(escolha) == int(1):
            processo_calcula_soma()
            escolha = 0

        elif int(escolha) == int(2):
            processo_calcula_media()
            escolha = 0

        elif int(escolha) == int(3):
            processo_calcula_raiz()
            escolha = 0

        elif int(escolha) == int(4):
            processo_calcula_numeros_primos()
            escolha = 0

        elif int(escolha) == int(0):
            sair = True

    print('\\n\\nPrograma Finalizado!')
